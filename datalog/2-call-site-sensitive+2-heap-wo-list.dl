  ArrayIndexPointsTo(?hctxInv1, ?hctxInv2, ?value, ?basehctxInv1, ?basehctxInv2, ?basevalue).
Stats_Simple_InsensInstanceFieldPointsTo(?value, ?sig, ?basevalue) :-
  InstanceFieldPointsTo(_, _, ?value, ?sig, _, _, ?basevalue).
Stats_Simple_InsensStaticFieldPointsTo(?value, ?sig) :-
